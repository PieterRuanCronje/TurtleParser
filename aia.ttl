                    owl:TransitiveProperty ,
                    owl:ReflexiveProperty ;
           rdfs:domain :Activity ,
                       :Procedure ;
           rdfs:range :Activity ,
                      :Procedure ;
           rdfs:comment "An Activity can be a single activity, or can comprise many activities (e.g. a program of activities)."@en ,
                        "EDIT"@en .


###  http://www.semanticweb.org/lucia/ontologies/2023/0/aia#contains
:contains rdf:type owl:ObjectProperty ;
          owl:inverseOf :isContainedIn ;
          rdfs:domain :Report ;
          rdfs:range :Claim ;
          rdfs:comment "EDIT"@en .


